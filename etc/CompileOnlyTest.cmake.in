cmake_minimum_required(VERSION @cetmodules_MIN_CMAKE_VERSION@...@CMAKE_VERSION@ FATAL_ERROR)

find_package(cetmodules @cetmodules_VERSION@ REQUIRED)
project(@CETMODULES_CURRENT_PROJECT_NAME@_COMPILE_ONLY_TEST
  LANGUAGES @CETMODULES_COMPILE_ONLY_TEST_ENABLED_LANGUAGES@)

include(CetCMakeEnv)
cet_cmake_env()

include(SetCompilerFlags)
cet_set_compiler_flags(DIAGS VIGILANT DWARF_VER 4
  WERROR NO_UNDEFINED EXTRA_FLAGS -pedantic)

find_package(@CETMODULES_CURRENT_PROJECT_NAME@
  @CETMODULES_CURRENT_PROJECT_VERSION@
  REQUIRED)

@CETMODULES_COMPILE_ONLY_TEST_COMPILE_COMMANDS@
